<expt>
/ blocks = [1 = block1; 2 = block2; 3 = block3; 4 = block4; 5 = block5; 6 = block6; 7 = block7; 8 = block8]
</expt>

<variables>
/group = (1 of 1) (block1 = Color_Contingency_Awareness_COLORS; block2 = Contingency_Awareness; block3 = Writing_Down_Contingencies; block4 = color_influence_awareness;  block5 = Color_Contingency_Awareness_CS_US; block6 = Demand; block7 = Reactance; Block8 = issues)
</variables>

<defaults>
/ canvassize = (100%, 100%)
/ screencolor = white
/ txcolor = black
/ fontstyle = ("Arial", 20%)
/ inputdevice = mouse
</defaults>

<survey Contingency_Awareness>
/ pages = [1 = awareness_1; 2 = awareness_2] 
/ txcolor = Black
/ fontstyle = ("Arial", 2.5%, false)
/ showpagenumbers = false
/ showquestionnumbers = false
/ showprogressbar = true
</survey>

<surveypage awareness_1>
/caption = "In the second part of the experiment we presented MORAG along with two other words. Did...?"
/ showpagenumbers = false 
/showquestionnumbers = false
/questions = [1=awareness_1]
</surveypage>

<radiobuttons awareness_1>
/options = ("Those two other words always have a positive meaning", "Those two other words always have a negative meaning", "One word always have a positive meaning and the other a negative meaning", "I don’t remember")
/ orientation = vertical
/ required=true
</radiobuttons>

<surveypage awareness_2>
/caption = "In the second part of the experiment we presented STRUAN along with two other words. Did...?"
/ showpagenumbers = false 
/showquestionnumbers = false
/questions = [1=awareness_2]
</surveypage>

<radiobuttons awareness_2>
/options = ("Those two other words always have a positive meaning", "Those two other words always have a negative meaning", "One word always have a positive meaning and the other a negative meaning", "I don’t remember")
/ orientation = vertical
/ required=true
</radiobuttons>

