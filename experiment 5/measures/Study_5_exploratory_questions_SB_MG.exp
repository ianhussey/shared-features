/ txcolor = (0,0,0)
/ required = true
/ fontstyle = ("ARIAL", 2.5%, false, false, false, false, 5, 0)
/textboxsize = (50,30)
/ position = (5%, 5%)
</textbox>

<survey size_influence_awareness>
